{
  "name": "qd-app-copilot",
  "version": "1.0.7",
  "description": "A support lib for quick dine",
  "main": "lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    },
    "./validation": "./lib/validations/index.js"
  },
  "files": [
    "lib",
    "esm",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "build": "tsc",
    "publish": "npm publish --access public",
    "patch": "npm version --patch && git push --follow-tags",
    "release-dev": "npm version prerelease --preid=dev && npm publish --tag=dev",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iam-hussain/qd-copilot.git"
  },
  "keywords": [
    "copilot",
    "lib",
    "package"
  ],
  "author": "iam-hussain",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iam-hussain/qd-copilot/issues"
  },
  "homepage": "https://github.com/iam-hussain/qd-copilot#readme",
  "devDependencies": {
    "rimraf": "^5.0.5",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "zod": "^3.22.4"
  }
}
